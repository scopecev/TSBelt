{
	"name": "tt-backend",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"clean": "rimraf '{src,test}/**/*.{js,js.map}'",
		"build": "npm run tsc",
		"test": "npm run test:lint && npm run test:unit",
		"test:unit": "cross-env NODE_ENV=test nyc --reporter=html --reporter=text mocha --recursive",
		"test:lint": "tslint --project tsconfig.json",
		"test:lint:fix": "tslint --project tsconfig.json --fix",
		"travis:deploy-once": "travis-deploy-once",
		"travis:coveralls": "nyc report --reporter=text-lcov | coveralls",
		"tsc": "tsc --project tsconfig.json",
		"tsc:w": "tsc --project tsconfig.json -w",
		"nlp:download": "npm explore corenlp -- npm run corenlp:download",
		"nlp:run": "npm explore corenlp -- npm run corenlp:server",
		"start": "ts-node src/index.ts",
		"start:dev": "nodemon --watch 'src/**/*.ts' --ignore 'node_modules/**/*' --exec npm start"
	},
	"author": "Samer Aldefai",
	"license": "MIT",
	"dependencies": {
		"@tsed/common": "5.6",
		"@tsed/core": "5.6",
		"@tsed/di": "5.6",
		"@tsed/swagger": "5.6",
		"@tsed/testing": "5.6",
		"@types/swagger-schema-official": "^2.0.20",
		"body-parser": "^1.19.0",
		"compression": "^1.7.4",
		"compromise": "^11.14.3",
		"concurrently": "^3.5.1",
		"cookie-parser": "^1.4.4",
		"corenlp": "^1.5.1",
		"express": "^4.17.1",
		"method-override": "^2.3.10",
		"mulang": "^5.1.0",
		"node-uuid": "^1.4.8",
		"ts-json-properties": "1.2.0",
		"tsd-jsdoc": "^2.4.0"
	},
	"devDependencies": {
		"@types/chai": "^4.2.7",
		"@types/chai-as-promised": "^7.1.2",
		"@types/express": "^4.17.2",
		"@types/http-proxy": "^1.17.2",
		"@types/mocha": "^5.2.7",
		"@types/node": "^10.17.12",
		"@types/request-promise": "^4.1.45",
		"@types/sinon": "^5.0.7",
		"@types/sinon-chai": "^2.7.36",
		"@types/supertest": "^2.0.8",
		"chai": "^4.1.2",
		"chai-as-promised": "^7.1.1",
		"mocha": "^5.2.0",
		"nodemon": "^1.19.4",
		"nyc": "^12.0.1",
		"rimraf": "^2.7.1",
		"sinon": "^6.3.5",
		"sinon-chai": "^3.4.0",
		"supertest": "^3.1.0",
		"ts-node": "^6.2.0",
		"tslint": "^5.20.1",
		"typescript": "^3.7.4",
		"wtf_wikipedia": "^7.8.1"
	}
}
