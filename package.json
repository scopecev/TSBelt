{
	"name": "tt-backend",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"clean": "rimraf '{src,test}/**/*.{js,js.map}'",
		"build": "npm run tsc",
		"test": "npm run test:lint && npm run test:unit",
		"test:unit": "cross-env NODE_ENV=test nyc --reporter=html --reporter=text mocha --recursive",
		"test:lint": "tslint --project tsconfig.json",
		"test:lint:fix": "tslint --project tsconfig.json --fix",
		"travis:deploy-once": "travis-deploy-once",
		"travis:coveralls": "nyc report --reporter=text-lcov | coveralls",
		"tsc": "tsc --project tsconfig.json",
		"tsc:w": "tsc --project tsconfig.json -w",
		"start": "ts-node src/index.ts",
		"start:dev": "nodemon --watch 'src/**/*.ts' --ignore 'node_modules/**/*' --exec ts-node src/index.ts"
	},
	"author": "Samer Aldefai",
	"license": "MIT",
	"dependencies": {
		"@tsed/common": "5.1.1",
		"@tsed/core": "5.1.1",
		"@tsed/di": "5.1.1",
		"@tsed/swagger": "5.1.1",
		"@tsed/testing": "5.1.1",
		"@types/swagger-schema-official": "^2.0.9",
		"body-parser": "^1.18.3",
		"compression": "^1.7.1",
		"compromise": "^11.13.2",
		"concurrently": "^3.5.1",
		"cookie-parser": "^1.4.3",
		"express": "^4.16.4",
		"method-override": "^2.3.10",
		"node-uuid": "^1.4.8",
		"ts-json-properties": "1.2.0"
	},
	"devDependencies": {
		"@types/chai": "^4.1.2",
		"@types/chai-as-promised": "^7.1.0",
		"@types/express": "^4.16.1",
		"@types/http-proxy": "^1.16.2",
		"@types/mocha": "^5.2.1",
		"@types/node": "^10.3.0",
		"@types/request-promise": "^4.1.41",
		"@types/sinon": "^5.0.7",
		"@types/sinon-chai": "^2.7.32",
		"@types/supertest": "^2.0.4",
		"chai": "^4.1.2",
		"chai-as-promised": "^7.1.1",
		"mocha": "^5.2.0",
		"nodemon": "^1.17.5",
		"nyc": "^12.0.1",
		"rimraf": "^2.6.2",
		"sinon": "^6.3.5",
		"sinon-chai": "^3.1.0",
		"supertest": "^3.1.0",
		"ts-node": "^6.0.5",
		"tslint": "^5.12.0",
		"typescript": "^3.3.3"
	}
}
